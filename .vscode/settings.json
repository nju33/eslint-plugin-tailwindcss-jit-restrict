{
	"[javascript]": {
		"editor.defaultFormatter": "dbaeumer.vscode-eslint"
	},
	"[javascriptreact]": {
		"editor.defaultFormatter": "dbaeumer.vscode-eslint"
	},
	"[json]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[shellscript]": {
		"editor.defaultFormatter": "foxundermoon.shell-format"
	},
	"[typescript]": {
		"editor.defaultFormatter": "dbaeumer.vscode-eslint"
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "dbaeumer.vscode-eslint"
	},
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.formatOnPaste": true,
	"editor.formatOnSave": true,
	"emmet.includeLanguages": {
		"javascript": "javascriptreact",
		"typescript": "typescriptreact"
	},
	"eslint.enable": false,
	"eslint.format.enable": true,
	"eslint.packageManager": "yarn",
	"files.associations": {
		".*.js": "javascript",
		".{husky,lintstaged,renovate,stylelint,all-contributors}rc": "json"
	},
	"files.exclude": {
		"**/.gitpod": true
	},
	"files.watcherExclude": {
		"**/.gitpod/**": true
	},
	"javascript.suggestionActions.enabled": false,
	"jest.autoEnable": false,
	"npm.packageManager": "yarn",
	"peacock.affectStatusBar": false,
	"peacock.color": "transparent",
	"peacock.remoteColor": "transparent",
	"plantuml.render": "PlantUMLServer",
	"plantuml.server": "http://localhost:8080",
	"search.exclude": {
		"**/.yarn": true,
		"**/.pnp.*": true
	},
	"stylelint.enable": false,
	"tslint.packageManager": "yarn",
	"workbench.colorCustomizations": {
		"activityBar.activeBackground": "#1a1a1a00",
		"activityBar.activeBorder": "#606020",
		"activityBar.background": "#1a1a1a00",
		"activityBar.foreground": "#e7e7e7",
		"activityBar.inactiveForeground": "#e7e7e799",
		"activityBarBadge.background": "#606020",
		"activityBarBadge.foreground": "#e7e7e7",
		"titleBar.activeBackground": "#00000000",
		"titleBar.activeForeground": "#e7e7e7",
		"titleBar.inactiveBackground": "#00000099",
		"titleBar.inactiveForeground": "#e7e7e799"
	}
}
